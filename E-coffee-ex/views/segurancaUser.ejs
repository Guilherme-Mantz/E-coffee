        <h1>Acesso e Segurança</h1>
    </div>
    <div class="security-options">
        <div class="acessibilidade">
            <p style="font-weight: bold;">Nome:</p>
            <div class="nome">
                <p><%= sessao.name %></p>
            </div>
            <div class="editar">
                <a href="#">Editar</a>
            </div>
        </div>
        <div class="acessibilidade">
            <p style="font-weight: bold;">CPF:</p>
            <div class="nome">
                <p style="margin-left: 13px;">***.***.***-**</p>
            </div>
            <div class="editar">
                <a href="#">Editar</a>
            </div>
        </div>
        <div class="acessibilidade">
            <p style="font-weight: bold;">E-mail:</p>
            <div class="nome">
                <p><%= sessao.email %></p>
            </div>
            <div class="editar">
                <a href="#">Editar</a>
            </div>
        </div>
        <div class="acessibilidade">
            <p style="font-weight: bold;">Senha:</p>
            <div class="nome">
                <p>***************</p>
            </div>
            <div class="editar">
                <a href="#">Editar</a>
            </div>
        </div>
        <div class="concluido">
            <a href="/paginadousuario">Concluído</a>
        </div>
